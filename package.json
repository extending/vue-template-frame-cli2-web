  "description": "vue-cli template frame for web",
  "author": "吕瑶 <lvyao91@sina.com>",
  "private": true,
  "scripts": {
    "mock": "json-server --watch ./src/mock/index.js --routes ./src/mock/routes.json -p 3000",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "concurrently \"npm run mock\" \"npm run dev\"",
    "start2": "npm run mock & npm run dev",
    "lint": "eslint --ext .js,.vue src/pages",
    "lint:fix": "echo lint begin--- && eslint --ext .js,.vue --fix src/pages && git add -A",
    "build": "npm run lint:fix && node build/build.js",
    "pre-commit": "echo pre-commit begin---"
  },
  "pre-commit": [
    "pre-commit",
    "lint:fix"
  ],
